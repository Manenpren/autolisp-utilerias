(defun C:DY1B ()
    (setq PUNTO (getpoint " ELEVACION A TOMAR : "))
    (setq PUNTO2 (getpoint " PUNTO DESTINO : "))
    (SETQ Y (CADR PUNTO))
    (SETQ Y (RTOS Y 2 3))
    (COMMAND "INSERT" (STRCAT RUTA "BLOCKS\\PUN2") PUNTO2 "" "" "")
    (SETQ BLOCK (ENTLAST))
    (SETQ BLOCK (ENTNEXT BLOCK))
    (COMMAND "ATTEDIT" "" "" "" "" BLOCK "V" "R" "SECCION" "")
    (SETQ BLOCK (ENTNEXT BLOCK))
    (COMMAND "ATTEDIT" "" "" "" "" BLOCK "V" "R" Y "")
    (SETQ BLOCK (ENTNEXT BLOCK))
    (COMMAND "ATTEDIT" "" "" "" "" BLOCK "V" "R" "1" "")
)

(defun C:DX1B ()
    (setq PUNTO (getpoint " ELEVACION A TOMAR : "))
    (setq BLOCK (car (entsel "Selecciona el BLOCK: ")))
    (SETQ X (CAR PUNTO))
    (SETQ X (RTOS X 2 0))
    (SETQ BLOCK (ENTNEXT BLOCK))
    (SETQ BLOCK (ENTNEXT BLOCK))
    (COMMAND "ATTEDIT" "" "" "" "" BLOCK "V" "R" Y "")
)
