(defun C:DY1 ()
    (setq PUNTO (getpoint " ELEVACION A TOMAR : "))
    (setq texto (car (entsel "Selecciona el texto: ")))
    (SETQ Y (CADR PUNTO))
    (SETQ Y (RTOS Y 2 3))
    (SETQ LNG (STRLEN y))
    (setq lng (1+ lng))
    (if (= (substr y (- lng 4) 4) ".000") (setq y (substr y 1 (- lng 5))))
    (COMMAND "CHANGE" texto "" "" "" "" "" "" (strcat "N=" Y))
)

